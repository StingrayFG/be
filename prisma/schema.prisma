generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model file {
  name            String
  uploadDate      DateTime? @db.Timestamptz(3)  @default(dbgenerated("NOW()")) 

  uuid            String    @id
  hashSum         String?

  parentFolderId  String 
  ownerId
}

model user {
  id
  login         String    @id
  password      String 
}
